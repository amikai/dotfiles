{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "YOUR_API_KEY"
      }
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--open-web-dashboard",
        "false"
      ],
      "env": {}
    },
    "sequentialthinking": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {}
    },
    "awslabs.cloudwatch-mcp-server": {
      "autoApprove": [],
      "disabled": false,
      "command": "uvx",
      "args": [
        "awslabs.cloudwatch-mcp-server@latest"
      ],
      "transportType": "stdio"
    },
    "codex": {
      "command": "bunx",
      "args": [
        "@openai/codex",
        "mcp-server",
        "-c",
        "sandbox_permissions=[\"disk-full-read-access\"]"
      ]
    }
  }
}
