{
    "$schema": "https://opencode.ai/config.json",
    "permission": {
        "bash": {
            "ls": "allow",
            "ls *": "allow",
            "pwd": "allow",
            "which *": "allow",
            "git *": "allow",
            "npm *": "allow",
            "npx *": "allow",
            "node *": "allow",
            "go *": "allow",
            "gh issue list": "allow",
            "gh issue list *": "allow",
            "gh issue view": "allow",
            "gh issue view *": "allow",
            "gh pr list": "allow",
            "gh pr list *": "allow",
            "gh pr view": "allow",
            "gh pr view *": "allow",
            "gh pr diff": "allow",
            "gh pr diff *": "allow",
            "git push": "ask",
            "git push *": "ask",
            "gh pr create": "ask",
            "gh pr create *": "ask",
            "gh pr merge *": "ask",
            "gh pr close *": "ask",
            "gh pr edit *": "ask",
            "gh pr comment *": "ask",
            "gh pr review *": "ask",
            "gh issue create": "ask",
            "gh issue create *": "ask",
            "gh issue close *": "ask",
            "gh issue edit *": "ask",
            "gh issue comment *": "ask",
            "gh issue delete *": "ask",
            "gh release create *": "ask",
            "gh release delete *": "ask",
            "gh release edit *": "ask",
            "gh repo create *": "ask",
            "gh repo delete *": "ask",
            "gh repo edit *": "ask",
            "gh repo fork *": "ask",
            "gh gist create *": "ask",
            "gh gist delete *": "ask",
            "gh gist edit *": "ask"
        },
        "read": "allow",
        "edit": "allow",
        "glob": "allow",
        "grep": "allow",
        "list": "allow",
        "task": "allow",
        "skill": "allow",
        "lsp": "allow",
        "webfetch": "allow",
        "websearch": "allow",
        "codesearch": "allow",
        "external_directory": "ask",
        "serena_*": "allow",
        "serena_execute_shell_command": "ask"
    },
    "plugin": [
        "opencode-antigravity-auth@latest",
        "@rama_nigg/open-cursor@latest"
    ],
    "mcp": {
        "context7": {
            "type": "remote",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "CONTEXT7_API_KEY": "YOUR_API_KEY"
            }
        },
        "serena": {
            "type": "local",
            "command": [
                "uvx",
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server",
                "--open-web-dashboard",
                "false"
            ],
            "environment": {}
        },
        "sequentialthinking": {
            "type": "local",
            "command": [
                "bunx",
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ],
            "environment": {}
        },
        "awslabs.cloudwatch-mcp-server": {
            "type": "local",
            "command": [
                "uvx",
                "awslabs.cloudwatch-mcp-server@latest"
            ],
            "environment": {}
        },
    },
